{
	"include": [
		"src"
	],
	"exclude": [
		"node_modules",
		"tmp"
	],
	"compilerOptions": {
		"outDir": "./dist",
		"target": "ESNext",
		"skipLibCheck": true,
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"esModuleInterop": true,
		"strict": true,
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx"
	}
}
